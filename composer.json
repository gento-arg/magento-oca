{
    "name": "gento-arg/module-oca",
    "description": "Modulo de Magento 2 para la integracion con OCA",
    "type": "magento2-module",
    "require": {
        "php": ">7.4",
        "ext-dom": "20031129",
        "ext-libxml": "*",
        "gento-arg/module-core": "*",
        "magento/framework": "*.*",
        "magento/module-backend": "*",
        "magento/module-config": "*",
        "magento/module-ui": "*"
    },
    "suggest": {
        "magento/module-graph-ql": "For use graphql endpoints"
    },
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "Gento\\Oca\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
